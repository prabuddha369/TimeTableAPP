<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back"
    tools:context=".MainActivity"
    tools:ignore="ExtraText">

    <TextView
        android:id="@+id/textViewResult"
        android:layout_width="327dp"
        android:layout_height="213dp"
        android:text=""
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button9"
        app:layout_constraintVertical_bias="0.145" />

    <Button
        android:id="@+id/button2"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:layout_marginEnd="20dp"
        android:text="@string/_10_50_11_40_am"
        android:textColor="#090808"
        app:backgroundTint="#004DBAC7"
        app:layout_constraintBottom_toTopOf="@+id/button4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner"
        app:layout_constraintVertical_bias="1.0"
        app:strokeColor="#000000" />

    <Button
        android:id="@+id/button3"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:text="11:40-12:30 PM"
        android:textColor="#090808"
        app:backgroundTint="#004DBAC7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.336"
        app:strokeColor="#000000" />

    <Button
        android:id="@+id/button4"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:layout_marginEnd="20dp"
        android:text="@string/_12_30_1_20_pm"
        android:textColor="#090808"
        app:backgroundTint="#00FFEB3B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.336"
        app:strokeColor="#000000" />

    <Button
        android:id="@+id/button5"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:text="@string/_1_20_2_10_pm"
        android:textColor="#090808"
        app:backgroundTint="#004DBAC7"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        app:layout_constraintVertical_bias="0.5"
        app:strokeColor="#000000" />

    <Button
        android:id="@+id/button6"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:layout_marginEnd="20dp"
        android:text="@string/_2_10_3_00_pm"
        android:textColor="#090808"
        app:backgroundTint="#00FFEB3B"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        app:layout_constraintVertical_bias="1.0"
        app:strokeColor="#000000" />

    <Button
        android:id="@+id/button7"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:text="@string/_3_00_3_50_pm"
        android:textColor="#090808"
        app:backgroundTint="#00FFEB3B"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button5"
        app:strokeColor="#000000" />

    <Button
        android:id="@+id/button8"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:layout_marginEnd="20dp"
        android:text="@string/_3_50_4_40_pm"
        android:textColor="#090808"
        app:backgroundTint="#00FFEB3B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button6"
        app:strokeColor="#000000" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dev@PrabuddhaChatterjee"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="210dp"
        android:layout_height="50dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/spinnerB"
        app:layout_constraintVertical_bias="0.059">

        <RadioButton
            android:id="@+id/radioButton1"
            android:layout_width="106dp"
            android:layout_height="wrap_content"
            android:checked="true"
            android:text="Grp X" />

        <RadioButton
            android:id="@+id/radioButton2"
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:text="Grp Y" />

    </RadioGroup>

    <Button
        android:id="@+id/button1"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:text="10:00-10:50 AM"
        android:textColor="#000000"
        app:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner"
        app:layout_constraintVertical_bias="1.0"
        app:strokeColor="#000000" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="270dp"
        android:layout_height="38dp"
        android:layout_marginTop="130dp"
        android:background="#74000000"
        android:spinnerMode="dropdown"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:layout_width="15dp"
        android:layout_height="9dp"
        android:layout_weight="1"
        app:layout_constraintBottom_toBottomOf="@+id/spinner"
        app:layout_constraintEnd_toEndOf="@+id/spinner"
        app:layout_constraintHorizontal_bias="0.882"
        app:layout_constraintStart_toStartOf="@+id/spinner"
        app:layout_constraintTop_toTopOf="@+id/spinner"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <Spinner
        android:id="@+id/spinnerB"
        android:layout_width="132dp"
        android:layout_height="39dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.537"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button9"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:text="4:40 - 5:30 PM "
        android:textAllCaps="false"
        android:textColor="#000000"
        app:backgroundTint="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7" /><![CDATA[









    />




]]>
</androidx.constraintlayout.widget.ConstraintLayout>